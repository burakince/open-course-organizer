<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field="nameLabel" />
			<g:Label ui:field="eventName" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field="locationLabel" />
			<g:Label ui:field="eventLocation" />
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>